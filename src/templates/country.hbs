{{#each this}}
<h2>{{name}}</h2>
<div style="display: flex; justify-content: centleer;">
  <div style="margin-right: 50px; font-weight: 600; font-size: 20px">
    <p>Capital {{capital}}</p>
    <p>Population {{population}}</p>
    <p>Langviges</p>
    <ul>
      {{#each languages}}
      <li>{{name}}</li>
      {{/each}}
    </ul>
  </div>
  <img src="{{flag}}" alt="{{name}}" style="width: 300px">
</div>
{{/each}}